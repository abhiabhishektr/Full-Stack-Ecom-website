            <script>
              // Function to update filters and trigger the filter immediately
              function updateFilters(searchQuery) {
                  // Get selected categories, sizes, and brands
                  
                  const selectedCategories = getSelectedCheckboxes('cat');
                  const selectedSizes = getSelectedCheckboxes('size');
                  const selectedBrands = getSelectedCheckboxes('brand');
          if (searchQuery==null || searchQuery=='[object Event]'){
            searchQuery= document.getElementById('q').value;
            console.log("hai",searchQuery)
          }
                 // const searchQueryTemp 

                  // Get selected sort option
                  const selectedSortOption = document.getElementById('sortby').value;
          
                  const currentUrl = window.location.href;
                  let urlRoot;
          
                  if (currentUrl.includes('/fullpdt/Men')) {
                      urlRoot = 'Men';
                  } else if (currentUrl.includes('/fullpdt/Women')) {
                      urlRoot = 'Women';
                  } else if (currentUrl.includes('/fullpdt/Kids')) {
                      urlRoot = 'Kids';
                  } else {
                      urlRoot = '0';
                  }
                  // Construct the URL with selected filters and sort option
                  const url = `/fullpdt/${urlRoot}?categories=${selectedCategories.join(',')}&sizes=${selectedSizes.join(',')}&brands=${selectedBrands.join(',')}&sortby=${selectedSortOption}&search=${encodeURIComponent(searchQuery)}`;
          
                  // Store the state of the checkboxes in local storage
                  localStorage.setItem('selectedCategories', JSON.stringify(selectedCategories));
                  localStorage.setItem('selectedSizes', JSON.stringify(selectedSizes));
                  localStorage.setItem('selectedBrands', JSON.stringify(selectedBrands));
          
                  // Redirect or make an AJAX request with the constructed URL
                  window.location.href = url; // or use fetch() for AJAX request
              }
          
              // Function to get selected checkboxes by a given prefix
              function getSelectedCheckboxes(prefix) {
                  const checkboxes = document.querySelectorAll(`input[id^=${prefix}-]:checked`);
                  return Array.from(checkboxes).map(checkbox => checkbox.id.replace(`${prefix}-`, ''));
              }
          
              // Function to restore the state of the checkboxes
              function restoreCheckboxState() {
                  // Get the state of the checkboxes from local storage
                  const selectedCategories = JSON.parse(localStorage.getItem('selectedCategories')) || [];
                  const selectedSizes = JSON.parse(localStorage.getItem('selectedSizes')) || [];
                  const selectedBrands = JSON.parse(localStorage.getItem('selectedBrands')) || [];
          
                  // Check the checkboxes that were previously selected
                  selectedCategories.forEach(category => document.getElementById(`cat-${category}`).checked = true);
                  selectedSizes.forEach(size => document.getElementById(`size-${size}`).checked = true);
                  selectedBrands.forEach(brand => document.getElementById(`brand-${brand}`).checked = true);
          
                  // Get the state of the sortby parameter from the URL
                  const urlParams = new URLSearchParams(window.location.search);
                  const search = urlParams.get('search');
                  const sortbyParam = urlParams.get('sortby');
          
                  // Set the selected option in the sortby dropdown
                  const sortbyDropdown = document.getElementById('sortby');
                  if (sortbyParam) {
                      // Check if the option exists before setting it
                      const option = sortbyDropdown.querySelector(`option[value="${sortbyParam}"]`);
                      if (option) {
                          option.selected = true;
                      }
                  }
                  // Searching
                  if (search !== null) {
                      document.getElementById('q').value = search;
                  }
              }
          
              // Add event listeners to checkboxes and sort select
              document.querySelectorAll('input[type=checkbox]').forEach(checkbox => {
                  checkbox.addEventListener('change', updateFilters);
              });
          
              document.getElementById('sortby').addEventListener('change', updateFilters);
          
              document.querySelectorAll('.page-link').forEach(pageLink => {
                  pageLink.addEventListener('click', event => {
                      event.preventDefault();
                      const url = event.target.href;
                      window.location.href = url;
                  });
              });
          
              // Restore the state of the checkboxes when the page loads
              window.onload = restoreCheckboxState;
          
              // Function to handle form submission
              function searchProducts(event) {
                  event.preventDefault(); // Prevent the form from submitting traditionally
          
                  const searchQuery = document.getElementById('q').value;
                  // Update filters after form submission
                  updateFilters(searchQuery);
              }
          </script>
          
            
